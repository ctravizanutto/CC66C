<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<HBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="75.0" prefWidth="1100.0" style="-fx-background-color: #aeb1d7;"
      xmlns="http://javafx.com/javafx/17.0.2-ea"
      fx:controller="utfpr.cc66c.client.controllers.views.recruiter.CandidatesetHboxController">
    <Text fx:id="candidateName" strokeType="OUTSIDE" strokeWidth="0.0" text="CSS">
        <font>
            <Font name="JetBrains Mono Regular" size="52.0"/>
        </font>
    </Text>
    <Button mnemonicParsing="false" text="Choose" HBox.hgrow="ALWAYS" onMouseClicked="#chooseOnMouseClicked">
        <HBox.margin>
            <Insets left="20.0" top="20.0"/>
        </HBox.margin>
        <font>
            <Font name="JetBrains Mono Regular" size="12.0"/>
        </font>
    </Button>
</HBox>
